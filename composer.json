{
    "name": "wioex/php-sdk",
    "description": "Official PHP SDK for WioEX Financial Data API - Enterprise-grade client library for stocks, news, currency, and financial market data",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "wioex",
        "stocks",
        "financial",
        "api",
        "sdk",
        "market-data",
        "currency",
        "news",
        "trading"
    ],
    "authors": [
        {
            "name": "WioEX Team",
            "email": "api-support@wioex.com",
            "homepage": "https://wioex.com"
        }
    ],
    "homepage": "https://github.com/wioex/php-sdk",
    "support": {
        "email": "api-support@wioex.com",
        "docs": "https://docs.wioex.com",
        "issues": "https://github.com/wioex/php-sdk/issues"
    },
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "phpstan/phpstan": "^1.10",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "Wioex\\SDK\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wioex\\SDK\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "preferred-install": "dist"
    },
    "scripts": {
        "test": "phpunit",
        "test:coverage": "phpunit --coverage-html coverage",
        "phpstan": "phpstan analyse src --level=8",
        "cs:check": "phpcs --standard=PSR12 src",
        "cs:fix": "phpcbf --standard=PSR12 src"
    }
}
